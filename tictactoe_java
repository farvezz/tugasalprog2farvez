    private void menuStartActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        Random rand = new Random();
        try{
            Thread.sleep(500);
        }
        catch (InterruptedException e){
            e.printStackTrace();
        }
        if (rand.nextInt(2) == 0){
            player1 = true;
            txt.setText("Player 1's turn");
        }
        else{
            player2 = true;
            txt.setText("Player 2's turn");
        }

    }                                         

    private void txtActionPerformed(java.awt.event.ActionEvent evt) {                                    
        // TODO add your handling code here:
    }                                   

    private void btn3ActionPerformed(java.awt.event.ActionEvent evt) {                                     
        // TODO add your handling code here:
    if (txt.getText().equals("Player 1's turn") || txt.getText().equals("Player 2's turn")){
        if (player1 == true){
            if (btn3.getText().equals("")){
                btn3.setForeground(new Color(255,0,0)); 
                btn3.setFont(new Font("MV Boli", Font.BOLD, 120));
                btn3.setText("X");
                player1 = false;
                txt.setText("Player 2's turn");
                check();
            }
            else
                JOptionPane.showMessageDialog(null, "Choose the empty box", "Warning", 2);
        }
        else{
            if (btn3.getText().equals("")){
                btn3.setForeground(new Color(0,0,255)); 
                btn3.setFont(new Font("MV Boli", Font.BOLD, 120));
                btn3.setText("O");
                player1 = true;
                txt.setText("Player 1's turn");
                check();
            }
            else
                JOptionPane.showMessageDialog(null, "Choose the empty box", "Warning", 2);
        }
    }
    }                                    

